<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>HttpGetServlet</servlet-name>
		<servlet-class>com.servlet.HttpGetServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HttpGetServlet</servlet-name>
		<url-pattern>/get</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ShowConcertsServlet</servlet-name>
		<servlet-class>com.servlet.ShowConcertsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ShowConcertsServlet</servlet-name>
		<url-pattern>/show</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>EmptyDBServlet</servlet-name>
		<servlet-class>com.servlet.EmptyDBServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EmptyDBServlet</servlet-name>
		<url-pattern>/del</url-pattern>
	</servlet-mapping>

</web-app>
